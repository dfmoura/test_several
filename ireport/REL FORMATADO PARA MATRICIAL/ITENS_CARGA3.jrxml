	<queryString>
		<![CDATA[SELECT CAB.NUNOTA,PAR.CODPARC,
CAB.CODEMP,
PAR.RAZAOSOCIAL,
CID.NOMECID,
SUM(CAB.PESOBRUTO) AS PESO,
CASE WHEN CAB.CIF_FOB='C' THEN 'CIF'
     WHEN CAB.CIF_FOB='D' THEN 'Transp. Próprio Destinatário'
     WHEN CAB.CIF_FOB='F' THEN 'FOB'
     WHEN CAB.CIF_FOB='R' THEN 'Transp. Próprio Remetente'
     WHEN CAB.CIF_FOB='S' THEN 'Sem Frete'
     WHEN CAB.CIF_FOB='T' THEN 'Terceiros' END AS CIF_FOB


FROM TGFORD ORD INNER JOIN TGFCAB CAB ON (CAB.ORDEMCARGA=ORD.ORDEMCARGA)
                INNER JOIN TGFPAR PAR On (CAB.CODPARC=PAR.CODPARC)
                INNER JOIN TSICID CID ON (CID.CODCID=PAR.CODCID)

WHERE ORD.ORDEMCARGA IN (SELECT C.ORDEMCARGA FROM TGFCAB C WHERE C.NUNOTA = $P{NUNOTA})
AND CAB.TIPMOV='V'

GROUP BY ORD.ORDEMCARGA,PAR.CODPARC,PAR.RAZAOSOCIAL,CAB.CODEMP,CID.NOMECID,CIF_FOB,CAB.NUNOTA]]>
	</queryString>
