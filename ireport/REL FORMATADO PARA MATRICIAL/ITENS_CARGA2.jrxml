	<queryString>
		<![CDATA[SELECT SUM(ITE.QTDNEG) AS QTD,
CAST((SELECT V.CODVOL FROM TGFVOL V WHERE V.CODVOL=PRO.CODVOL) AS varchar(10)) AS UNP,
SUM(SANKHYA.VOLUMEALT_MOINHO(ITE.CODPROD,ITE.QTDNEG,ITE.CODVOL))  AS QTDALT,
CAST(ITE.CODVOL AS varchar(10)) AS UNALT,
CAB.ORDEMCARGA,
ITE.CODPROD,
PRO.DESCRPROD,
SUM(PRO.PESOLIQ * ITE.QTDNEG) AS PESO,
ITE.CONTROLE


FROM TGFCAB CAB INNER JOIN TGFTOP T ON (CAB.CODTIPOPER=T.CODTIPOPER AND CAB.DHTIPOPER=T.DHALTER)
                INNER JOIN TGFITE ITE On (CAB.NUNOTA=ITE.NUNOTA)
                INNER JOIN TGFPRO PRO ON (ITE.CODPROD=PRO.CODPROD)
                INNER JOIN TGFVOL VOL ON (VOL.CODVOL=PRO.CODVOL)


WHERE CAB.ORDEMCARGA IN (SELECT C.ORDEMCARGA FROM TGFCAB C WHERE C.NUNOTA = $P{NUNOTA})
AND CAB.CODEMP IN (SELECT C.CODEMP FROM TGFCAB C WHERE C.NUNOTA = $P{NUNOTA})
AND CAB.TIPMOV IN ('V','T')
AND ITE.USOPROD <> 'S'
AND ITE.SEQUENCIA>0
AND (T.ATUALEST <> 'N' OR T.ATUALESTMP<>0)
GROUP BY CAB.ORDEMCARGA,ITE.CODPROD,VOL.CODVOL,PRO.DESCRPROD,ITE.CONTROLE,ITE.CODVOL,PRO.CODVOL
ORDER BY PRO.DESCRPROD]]>
	</queryString>
